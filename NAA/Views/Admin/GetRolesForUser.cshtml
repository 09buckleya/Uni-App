
@{
    ViewBag.Title = "GetRolesForUser";
}

<h2>Get Roles For User</h2>
@using (Html.BeginForm("GetRolesForUser", "Admin"))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <p>
        User: @Html.DropDownList("UserName", (IEnumerable<SelectListItem>)ViewBag.Users)
    </p>
    <input type="submit" value="Get" />
}
